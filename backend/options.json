{
    "title" : "Justinian",
    "home" : {"module" : "support", "application" : "home"},
    "error" : {"module" : "core", "application" : "error"},
    "modules" : ["core", "lex", "court", "coi", "support"],
    "asset-extensions" : ["html", "js", "css", "png"],
    "sparql" : {
        "datastore-url" : "http://linked.opendata.cz/sparql",
        "query-param-name" : "query",
        "default-params": {
            "format" : "application/ld+json",
            "timeout" : "0",
            "debug" : "on"
        },
        "jsonld" : {
            "format": "application/ld+json",
            "empty-result": {
                "@graph": []
            },
            "error-result": {
                "@graph": [],
                "@error": []
            },
            "default-context": {}
        }
    },
    "solr" : {
    	"datastore-url" : "http://localhost:8983/solr/collection1/select",
    	"default-params": {
            "wt" : "json",
            "indent" : "true"
        }
    },
    "port" : 8000,
    "development" : true
}
