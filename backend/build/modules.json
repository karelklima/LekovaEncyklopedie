{
  "core": {
    "prefixes": {
      "frbr:": "http://purl.org/vocab/frbr/core#",
      "sdo:": "http://salt.semanticauthoring.org/ontologies/sdo#"
    },
    "apps": {
      "detail": {
        "datatypes": [
          "frbr:Expression"
        ],
        "views": [
          "main",
          "sidebar"
        ]
      },
      "search": {
        "views": [
          "main"
        ]
      },
      "search-form": {
        "views": [
          "search"
        ]
      }
    }
  },
  "pis": {
    "extends": [
      "core"
    ],
    "prefixes": {
      "lex:": "http://purl.org/lex#",
      "lexcz:": "http://purl.org/lex/cz#"
    },
    "apps": {
      "act-detail": {
        "datatypes": [
          "frbr:Expression"
        ],
        "views": [
          "main",
          "sidebar"
        ]
      },
      "search": {
        "views": [
          "main"
        ]
      }
    }
  }
}